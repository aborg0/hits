<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>
   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   <extension point="org.knime.workbench.repository.nodes">
      <node
            category-path="/"
            expert-flag="true"
            factory-class="ie.tcd.imm.hits.image.INCellImageReaderNodeFactory"
            id="ie.tcd.imm.hits.image.INCellImageReaderNodeFactory"/>
      <node category-path="/" factory-class="ie.tcd.imm.hits.image.loci.read.LociReaderNodeFactory" id="ie.tcd.imm.hits.image.ome.read.OMEReaderNodeFactory"/>
      <node category-path="/" factory-class="ie.tcd.imm.hits.image.loci.view.LociViewerNodeFactory" id="ie.tcd.imm.hits.image.ome.view.OMEViewerNodeFactory"/>
   </extension>
</plugin>
