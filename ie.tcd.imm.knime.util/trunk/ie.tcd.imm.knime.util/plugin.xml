<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/" factory-class="ie.tcd.imm.knime.util.pivot.PivotNodeFactory" id="ie.tcd.imm.knime.util.pivot.PivotNodeFactory"/>
   <node category-path="/" factory-class="ie.tcd.imm.knime.util.unpivot.UnpivotNodeFactory" id="ie.tcd.imm.knime.util.unpivot.UnpivotNodeFactory"/>
</extension>
   
</plugin>
