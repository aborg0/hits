<?xml version="1.0" encoding="UTF-8"?>
<p:root
	xmlns:p="http://hits.googlecode.com/svn/ie.tcd.imm.hits.interop/trunk/ie.tcd.imm.hits.interop"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://hits.googlecode.com/svn/ie.tcd.imm.hits.interop/trunk/ie.tcd.imm.hits.interop interop.xsd ">
  <p:defaults>
    <p:default p:column="Experiment" p:dialog="group" p:input="true">custom</p:default>
    <p:default p:column="Experiment" p:dialog="name" p:input="true">Experiment</p:default>
    <p:default p:column="Experiment" p:dialog="format" p:input="true">(.*)</p:default>
    <p:default p:column="Experiment" p:dialog="group" p:input="false">custom</p:default>
    <p:default p:column="Experiment" p:dialog="name" p:input="false">Experiment</p:default>
    <p:default p:column="Experiment" p:dialog="format" p:input="false">${Experiment}</p:default>
    <p:default p:column="Experiment" p:dialog="type" p:input="false">String</p:default>
    <p:default p:column="Plate" p:dialog="group" p:input="true">custom</p:default>
    <p:default p:column="Plate" p:dialog="name" p:input="true">Plate</p:default>
    <p:default p:column="Plate" p:dialog="format" p:input="true">(\d+)</p:default>
    <p:default p:column="Plate" p:dialog="group" p:input="false">custom</p:default>
    <p:default p:column="Plate" p:dialog="name" p:input="false">Plate</p:default>
    <p:default p:column="Plate" p:dialog="format" p:input="false">${Plate}</p:default>
    <p:default p:column="Plate" p:dialog="type" p:input="false">Integer</p:default>
    <p:default p:column="Replicate" p:dialog="group" p:input="true">custom</p:default>
    <p:default p:column="Replicate" p:dialog="name" p:input="true">Replicate</p:default>
    <p:default p:column="Replicate" p:dialog="format" p:input="true">(\d+)</p:default>
    <p:default p:column="Replicate" p:dialog="group" p:input="false">custom</p:default>
    <p:default p:column="Replicate" p:dialog="name" p:input="false">Replicate</p:default>
    <p:default p:column="Replicate" p:dialog="format" p:input="false">${Replicate}</p:default>
    <p:default p:column="Replicate" p:dialog="type" p:input="false">Integer</p:default>
    <p:default p:column="WellRow" p:dialog="group" p:input="true">custom</p:default>
    <p:default p:column="WellRow" p:dialog="name" p:input="true">Well</p:default>
    <p:default p:column="WellRow" p:dialog="format" p:input="true">^([A-P])(?:\s)?(?:-)?(?:\s)?(?:\d+)$</p:default>
    <p:default p:column="WellRow" p:dialog="group" p:input="false">custom</p:default>
    <p:default p:column="WellRow" p:dialog="name" p:input="false">Well</p:default>
    <p:default p:column="WellRow" p:dialog="format" p:input="false">${Well Row} - ${Well Column}</p:default>
    <p:default p:column="WellRow" p:dialog="type" p:input="false">String</p:default>
    <p:default p:column="WellColumn" p:dialog="group" p:input="true">custom</p:default>
    <p:default p:column="WellColumn" p:dialog="name" p:input="true">Well</p:default>
    <p:default p:column="WellColumn" p:dialog="format" p:input="true">^(?:[A-P])(?:\s)?(?:-)?(?:\s)?(\d+)$</p:default>
    <p:default p:column="WellColumn" p:dialog="group" p:input="false">custom</p:default>
    <p:default p:column="WellColumn" p:dialog="name" p:input="false">Well</p:default>
    <p:default p:column="WellColumn" p:dialog="format" p:input="false">${Well Row} - ${Well Column}</p:default>
    <p:default p:column="WellColumn" p:dialog="type" p:input="false">String</p:default>
  </p:defaults>
  <p:profiles>
    <p:profile p:name="custom" p:modifiable="true" p:input="true">
      <p:value p:column="Experiment" p:dialog="name">Experiment</p:value>
      <p:value p:column="Experiment" p:dialog="format">(.*)</p:value>
      <p:value p:column="Plate" p:dialog="name">Plate</p:value>
      <p:value p:column="Plate" p:dialog="format">(\d+)</p:value>
      <p:value p:column="Replicate" p:dialog="name">Replicate</p:value>
      <p:value p:column="Replicate" p:dialog="format">(\d+)</p:value>
      <p:value p:column="WellRow" p:dialog="name">Well</p:value>
      <p:value p:column="WellRow" p:dialog="format">^([A-P])(?:\s)?(?:-)?(?:\s)?(?:\d+)$</p:value>
      <p:value p:column="WellColumn" p:dialog="name">Well</p:value>
      <p:value p:column="WellColumn" p:dialog="format">^(?:[A-P])(?:\s)?(?:-)?(?:\s)?(\d+)$</p:value>
    </p:profile>
    <p:profile p:name="custom" p:modifiable="true" p:input="false">
      <p:value p:column="Experiment" p:dialog="name">Experiment</p:value>
      <p:value p:column="Experiment" p:dialog="format">${Experiment}</p:value>
      <p:value p:column="Experiment" p:dialog="type">String</p:value>
      <p:value p:column="Plate" p:dialog="name">Plate</p:value>
      <p:value p:column="Plate" p:dialog="format">${Plate}</p:value>
      <p:value p:column="Plate" p:dialog="type">Integer</p:value>
      <p:value p:column="Replicate" p:dialog="name">Replicate</p:value>
      <p:value p:column="Replicate" p:dialog="format">${Replicate}</p:value>
      <p:value p:column="Replicate" p:dialog="type">Integer</p:value>
      <p:value p:column="WellRow" p:dialog="name">Well</p:value>
      <p:value p:column="WellRow" p:dialog="format">${Well Row} - ${Well Column}</p:value>
      <p:value p:column="WellRow" p:dialog="type">String</p:value>
      <p:value p:column="WellColumn" p:dialog="name"></p:value>
      <p:value p:column="WellColumn" p:dialog="format"></p:value>
      <p:value p:column="WellColumn" p:dialog="type">Do not generate</p:value>
    </p:profile>
    <p:profile p:name="cellHTS2Input" p:modifiable="false" p:input="true">
      <p:value p:column="Experiment" p:dialog="name"></p:value>
      <p:value p:column="Experiment" p:dialog="format">test</p:value>
      <p:value p:column="Plate" p:dialog="name">Plate</p:value>
      <p:value p:column="Plate" p:dialog="format">(\d+)</p:value>
      <p:value p:column="Replicate" p:dialog="name">Replicate</p:value>
      <p:value p:column="Replicate" p:dialog="format">(\d+)</p:value>
      <p:value p:column="WellRow" p:dialog="name">Well</p:value>
      <p:value p:column="WellRow" p:dialog="format">^([A-P])(?:\s)?(?:-)?(?:\s)?(?:\d+)$</p:value>
      <p:value p:column="WellColumn" p:dialog="name">Well</p:value>
      <p:value p:column="WellColumn" p:dialog="format">^(?:[A-P])(?:\s)?(?:-)?(?:\s)?(\d+)$</p:value>
    </p:profile>
    <p:profile p:name="cellHTS2Input" p:modifiable="false" p:input="false">
      <p:value p:column="Experiment" p:dialog="name"></p:value>
      <p:value p:column="Experiment" p:dialog="format"></p:value>
      <p:value p:column="Experiment" p:dialog="type">Do not generate</p:value>
      <p:value p:column="Plate" p:dialog="name">Plate</p:value>
      <p:value p:column="Plate" p:dialog="format">${Plate}</p:value>
      <p:value p:column="Plate" p:dialog="type">Integer</p:value>
      <p:value p:column="Replicate" p:dialog="name">Replicate</p:value>
      <p:value p:column="Replicate" p:dialog="format">${Replicate}</p:value>
      <p:value p:column="Replicate" p:dialog="type">Integer</p:value>
      <p:value p:column="WellRow" p:dialog="name">Well</p:value>
      <p:value p:column="WellRow" p:dialog="format">${Well Row} - ${Well Column}</p:value>
      <p:value p:column="WellRow" p:dialog="type">String</p:value>
      <p:value p:column="WellColumn" p:dialog="name"></p:value>
      <p:value p:column="WellColumn" p:dialog="format"></p:value>
      <p:value p:column="WellColumn" p:dialog="type">Do not generate</p:value>
    </p:profile>
    <p:profile p:name="cellHTS2Output" p:modifiable="false" p:input="true" p:extends="cellHTS2Input">
      <p:value p:column="Experiment" p:dialog="name">Experiment</p:value>
      <p:value p:column="Experiment" p:dialog="format">(.*)</p:value>
      <p:value p:column="WellRow" p:dialog="format">^([A-P])\d\d$</p:value>
      <p:value p:column="WellColumn" p:dialog="format">^[A-P](\d\d)$</p:value>
    </p:profile>
    <p:profile p:name="cellHTS2Output" p:modifiable="false" p:input="false" p:extends="cellHTS2Input">
      <p:value p:column="Experiment" p:dialog="name">Experiment</p:value>
      <p:value p:column="Experiment" p:dialog="format">${Experiment}</p:value>
      <p:value p:column="Experiment" p:dialog="type">String</p:value>
      <p:value p:column="WellRow" p:dialog="format">${Well Row}${Well Column}</p:value>
    </p:profile>
    <p:profile p:name="HCDC" p:modifiable="false" p:input="true">
      <p:value p:column="Experiment" p:dialog="name">barcode</p:value>
      <p:value p:column="Experiment" p:dialog="format">(.*?)(?:\d+)_(?:\d+)$</p:value>
      <p:value p:column="Plate" p:dialog="name">barcode</p:value>
      <p:value p:column="Plate" p:dialog="format">.*?(\d+)_(?:\d+)$</p:value>
      <p:value p:column="Replicate" p:dialog="name">barcode</p:value>
      <p:value p:column="Replicate" p:dialog="format">^(?:.*?)_(\d+)$</p:value>
      <p:value p:column="WellRow" p:dialog="name">row</p:value>
      <p:value p:column="WellRow" p:dialog="format">^([A-P])$</p:value>
      <p:value p:column="WellColumn" p:dialog="name">col</p:value>
      <p:value p:column="WellColumn" p:dialog="format">^(\d+)$</p:value>
    </p:profile>
    <p:profile p:name="HCDC" p:modifiable="false" p:input="false">
      <p:value p:column="Experiment" p:dialog="name">barcode</p:value>
      <p:value p:column="Experiment" p:dialog="format">${Experiment}_${Plate}_${Replicate}</p:value>
      <p:value p:column="Experiment" p:dialog="type">String</p:value>
      <p:value p:column="Plate" p:dialog="name">barcode</p:value>
      <p:value p:column="Plate" p:dialog="format">${Experiment}_${Plate}_${Replicate}</p:value>
      <p:value p:column="Plate" p:dialog="type">String</p:value>
      <p:value p:column="Replicate" p:dialog="name">barcode</p:value>
      <p:value p:column="Replicate" p:dialog="format">${Experiment}_${Plate}_${Replicate}</p:value>
      <p:value p:column="Replicate" p:dialog="type">String</p:value>
      <p:value p:column="WellRow" p:dialog="name">row</p:value>
      <p:value p:column="WellRow" p:dialog="format">${Well Row}</p:value>
      <p:value p:column="WellRow" p:dialog="type">String</p:value>
      <p:value p:column="WellColumn" p:dialog="name">col</p:value>
      <p:value p:column="WellColumn" p:dialog="format">${Well Column}</p:value>
      <p:value p:column="WellColumn" p:dialog="type">Integer</p:value>
    </p:profile>
  </p:profiles>
</p:root>
